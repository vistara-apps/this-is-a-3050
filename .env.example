# FlowLink CRM Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api

# OAuth Integration Keys
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
VITE_SLACK_CLIENT_ID=your_slack_oauth_client_id_here
VITE_ZAPIER_CLIENT_ID=your_zapier_client_id_here

# Application Configuration
VITE_APP_NAME=FlowLink CRM
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_DEMO_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBHOOKS=true

# External Services (Optional)
VITE_SENTRY_DSN=your_sentry_dsn_for_error_tracking
VITE_MIXPANEL_TOKEN=your_mixpanel_token_for_analytics
VITE_INTERCOM_APP_ID=your_intercom_app_id_for_support

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
